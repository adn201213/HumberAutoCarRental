@model HumberAutoCarRental.Models.CarType

@{
    ViewBag.Title = "Detailes";
}
<div>
    <h3>Car Type Details</h3>
    <br />
    <div class="form-border">
        <fieldset disabled>
            <div class="row" style="padding-bottom:5px;padding-top:5px;">
                <div class="col-md-2">
                    @Html.LabelFor(m => m.Name, htmlAttributes: new { @class = "control-label" })
                </div>
                <div class="col-md-10">
                    @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
                </div>
            </div>
        </fieldset>
    </div>
</div>
<br />
<p>
    @Html.Partial("_BackToListButtonPartial")
    @Html.Partial("_EditButtonPartial", Model.Id)
</p>

