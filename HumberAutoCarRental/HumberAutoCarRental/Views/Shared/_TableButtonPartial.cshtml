@model HumberAutoCarRental.Models.IndividualButtonPartial
@using HumberAutoCarRental.Models



<td style="width:400px;">
    <div class="btn-group" role="group">
        @Html.Partial("_IndividualButtonPartial",
       new IndividualButtonPartial
   {
       Action = "Edit",
       ButtonType = "btn-primary",
       Glyph = "pencil",
       Text = "Edit Button",
       CartypeId = Model.CartypeId,
         CarId = Model.CarId,
       CustomerId = Model.CustomerId,
       MembershipTypeId = Model.MembershipTypeId,
       UserId = Model.UserId
   })
        @Html.Partial("_IndividualButtonPartial",
        new IndividualButtonPartial
        {
            Action = "Details",
            ButtonType = "btn-success",
            Glyph = "list",
            Text = "Details Button",
            CartypeId = Model.CartypeId,
            CarId = Model.CarId,
            CustomerId = Model.CustomerId,
            MembershipTypeId = Model.MembershipTypeId,
            UserId = Model.UserId

        })
        @Html.Partial("_IndividualButtonPartial",
           new IndividualButtonPartial
           {
               Action = "Delete",
               ButtonType = "btn-danger",
               Glyph = "trash",
               Text = "Delete Button",
               CartypeId = Model.CartypeId,
               CarId = Model.CarId,
               CustomerId = Model.CustomerId,
               MembershipTypeId = Model.MembershipTypeId,
               UserId = Model.UserId

           })

    </div>
</td>